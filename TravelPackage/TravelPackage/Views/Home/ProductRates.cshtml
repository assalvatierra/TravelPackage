@model IEnumerable<TravelPackage.Models.tpProdRate>

<div class="row">
    <h2>Rate</h2>
    @foreach (var item in Model)
    {
        <div class="col-md-2">
            <p>
                <span style="font-size:xx-small;color:red;">(@item.GroupOf pax)</span>
                <br />@item.Rate/@item.tpUom.Measure <br /></p>
        </div>
    }
    <hr />
</div>



