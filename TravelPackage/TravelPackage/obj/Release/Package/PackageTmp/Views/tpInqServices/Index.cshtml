@model IEnumerable<TravelPackage.Models.tpInqServices>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Back to List", "Index","tpInquiries", null, null)
</p>
<table class="table">
    <tr>
        <th>
           Date Start
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Message)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tpProduct.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tpInquiry.LeadGuest)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.dtSvcStart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Message)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tpProduct.tpArea.Name) -
            @Html.DisplayFor(modelItem => item.tpProduct.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tpInquiry.LeadGuest)
        </td>
    </tr>
}

</table>
